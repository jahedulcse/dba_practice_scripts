CREATE BIGFILE TABLESPACE TBS_FSMMS DATAFILE 
  '+DATA/PORTALDB/DATAFILE/tbs_fsmmsprod.dbf' SIZE 20G AUTOEXTEND ON NEXT 1G MAXSIZE 34359738344K
LOGGING
DEFAULT 
  NO INMEMORY
ONLINE
EXTENT MANAGEMENT LOCAL AUTOALLOCATE
BLOCKSIZE 8K
SEGMENT SPACE MANAGEMENT AUTO
FLASHBACK ON;
-------
CREATE BIGFILE TABLESPACE TBS_FSMMS_IDX DATAFILE 
  '+DATA/PORTALDB/DATAFILE/tbs_fsmmsprod_idx.dbf' SIZE 2G AUTOEXTEND ON NEXT 100M MAXSIZE 33554431M
LOGGING
DEFAULT 
  NO INMEMORY
ONLINE
EXTENT MANAGEMENT LOCAL AUTOALLOCATE
BLOCKSIZE 8K
SEGMENT SPACE MANAGEMENT AUTO
FLASHBACK ON;
-------

CREATE USER FSMMSPROD
  IDENTIFIED BY <password>
  DEFAULT TABLESPACE TBS_FSMMS
  TEMPORARY TABLESPACE TEMP
  PROFILE DEFAULT
  ACCOUNT UNLOCK;
  -- 1 Role for FSMMSPROD 
  GRANT DBA TO FSMMSPROD;
  ALTER USER FSMMSPROD DEFAULT ROLE ALL;
  -- 2 System Privileges for FSMMSPROD 
  GRANT CREATE SESSION TO FSMMSPROD;
  GRANT UNLIMITED TABLESPACE TO FSMMSPROD;
  -- 2 Tablespace Quotas for FSMMSPROD 
  ALTER USER FSMMSPROD QUOTA UNLIMITED ON TBS_FSMMS;
  ALTER USER FSMMSPROD QUOTA UNLIMITED ON TBS_FSMMS_IDX;
  -- 1 Object Privilege for FSMMSPROD 
    GRANT EXECUTE, READ, WRITE ON DIRECTORY BACKUP TO FSMMSPROD WITH GRANT OPTION;



