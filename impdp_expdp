============== foodprod gtl -------------
*****************************************************************

CREATE BIGFILE TABLESPACE FOODTBS#DATA DATAFILE 
  '+DATA/PORTALDB/DATAFILE/foodprod#data.ora' SIZE 250G AUTOEXTEND ON NEXT 1G MAXSIZE 34359738344K
LOGGING
DEFAULT 
  NO INMEMORY
ONLINE
EXTENT MANAGEMENT LOCAL AUTOALLOCATE
BLOCKSIZE 8K
SEGMENT SPACE MANAGEMENT AUTO
FLASHBACK ON;
-------

CREATE BIGFILE TABLESPACE FOODTBS#INDX DATAFILE 
  '+DATA/PORTALDB/DATAFILE/foodprod#indx.ora' SIZE 32G AUTOEXTEND ON NEXT 1G MAXSIZE 34359738344K
LOGGING
DEFAULT 
  NO INMEMORY
ONLINE
EXTENT MANAGEMENT LOCAL AUTOALLOCATE
BLOCKSIZE 8K
SEGMENT SPACE MANAGEMENT AUTO
FLASHBACK ON;
---------

CREATE USER FOODPROD
  IDENTIFIED BY <password>
  DEFAULT TABLESPACE TBS_FSMMS
  TEMPORARY TABLESPACE TEMP
  PROFILE DEFAULT
  ACCOUNT UNLOCK;
  -- 1 Role for FOODPROD 
  GRANT DBA TO FOODPROD;
  ALTER USER FOODPROD DEFAULT ROLE ALL;
  -- 2 System Privileges for FOODPROD 
  GRANT CREATE SESSION TO FOODPROD;
  GRANT UNLIMITED TABLESPACE TO FOODPROD;
  -- 2 Tablespace Quotas for FOODPROD 
  ALTER USER FOODPROD QUOTA UNLIMITED ON FOODTBS#DATA;
  ALTER USER FOODPROD QUOTA UNLIMITED ON FOODTBS#INDX;

**************

